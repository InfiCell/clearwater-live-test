<?xml version="1.0" encoding="UTF-8"?>
<ServiceProfile>
  <% ifcs.each do |ifc| %>
  <InitialFilterCriteria>
    <% if ifc[:priority] %><Priority><%= ifc[:priority] %></Priority><% end %>
    <TriggerPoint>
      <ConditionTypeCNF>0</ConditionTypeCNF>
      <SPT>
        <ConditionNegated>0</ConditionNegated>
        <Group>0</Group>
        <Method><% ifc[:method] ||= "INVITE" %><%= ifc[:method] %></Method>
        <Extension></Extension>
      </SPT>
      <% unless ifc[:session_case].nil? %>
       <SPT>
        <ConditionNegated>0</ConditionNegated>
        <Group>0</Group>
        <SessionCase><%= ifc[:session_case] %></SessionCase>
        <Extension></Extension>
      </SPT>
      <% end %>
      <% unless ifc[:req_uri].nil? %>
       <SPT>
        <ConditionNegated>0</ConditionNegated>
        <Group>0</Group>
        <RequestURI><%= ifc[:req_uri] %></RequestURI>
        <Extension></Extension>
      </SPT>
      <% end %>
   </TriggerPoint>
    <ApplicationServer>
      <ServerName>sip:<%= ifc[:server_name] %></ServerName>
      <DefaultHandling>0</DefaultHandling>
    </ApplicationServer>
  </InitialFilterCriteria>
  <% end %>
</ServiceProfile>
